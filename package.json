{
  "name": "issues2md",
  "version": "1.0.0",
  "description": "Export Github Issues (for bloggers) to markdown file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "issues2md": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "github",
    "issues",
    "markdown",
    "blog",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.16.0"
  },
  "packageManager": "pnpm@8.1.0",
  "dependencies": {
    "commander": "^9.4.1",
    "axios": "^1.1.3",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "files": [
    "dist"
  ]
}
